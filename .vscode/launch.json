{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "elements [debug]",
      "cargo": {
        "args": ["build"],
        "filter": {
          "name": "elements",
          "kind": "bin"
        }
      },
      "env": {
        "RUST_LOG": "info,wgpu=warn,naga=warn,quinn=warn,sentry=debug,sentry-anyhow=debug,sentry-core=debug,fbxcel=error",
        "DIMS_DISABLE_TIMEOUT": "true"
      },
      "args": ["--project-path=examples/primitives", "run"],
      "cwd": "${workspaceFolder}",
      "sourceLanguages": ["rust"]
    },
    {
      "type": "lldb",
